/* tablet & kleinere schermen */
/*-----------------------------------------------------------------------------------------------------*/	
@media only screen and (min-width: 150px) and (max-width: 1090px) 
{
	.mainheader nav {
	height: 48px;
	}
	
	.body {
		margin: 0 auto;
		width: 90%;
		clear: both;
		font-size: 95%;
	}
	
	.mainheader nav ul li {
		text-align: center;
	}
	
	.filmcont {
		margin-left: 7%;
	}
	
	.topbox {
	background-color: #00FFAD;
	width: 50%;
	margin-left: 25%;
	border-radius: 5px;
	}
	
	.navbar2 {
	padding-left: 35%;
	
}
	

	/* mobiele versie */
/*-----------------------------------------------------------------------------------------------------*/	
@media only screen and (min-width: 150px) and (max-width: 880px) 
{
	.mainheader nav {
		height: 335px;
	}
	
	.mainheader nav ul {
		padding-left: 0;
	}

	.mainheader nav ul li {
		width:100%;
		margin: 0 auto;
		text-align: center;
	}

	.mainheader nav a:link, .mainheader nav a:visited {
		padding: 10px 25px;
		height: 28px;
		display: block;
	}
	
	.filmcont {
		margin-left: 12%;
	}
	
	.navbar2 {
	
	
	margin-right: 10%;
	padding-right: 5%;
	}
}	
@media only screen and (min-width: 150px) and (max-width: 600px) {

	.navbar2 {
	margin:auto;
	padding-left: 18%;	
	
	
	}

}

