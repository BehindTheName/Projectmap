	<script>
	var d =  new Date();
	var N = d.getDay();
	function GaAgenda() {
		
	
		switch (N)
		{
		case 0 :  location.href="Agenda/Agenda-zo.html"; 0.
		break;
		case 1 :  location.href ="Agenda/Agenda-ma.html";
		break;
		case 2 :  location.href="Agenda/Agenda-di.html";
		break;
		case 3 :  location.href="Agenda/Agenda-wo.html";
		break;
		case 4 :  location.href="Agenda/Agenda-do.html";
		break;
		case 5 :  location.href="Agenda/Agenda-vr.html";
		break;
		case 6 :  location.href="Agenda/Agenda-za.html";
		break;
		}
	}

	function redirect(){
	document.location.href = "films/" + document.getElementById("search").value + ".html";
	return false;
	}

	</script> 

	<script>
	// Dit geeft het bericht dat de email is verzonden, maar dit is niet zo!
	//Er wordt dus geen email verzonden.
	function Verzend(){
		var Voornaam = document.getElementById("Voornaam").value;
		var Achternaam = document.getElementById("Achternaam").value;
		var Telefoonnummer = document.getElementById("Telefoonnummer").value;
		var Emailadres = document.getElementById("Email").value;
		var Bericht = document.getElementById("Tekst").value;
		
		document.getElementById("form-wrap").innerHTML = "";
		document.getElementById("form-wrap").innerHTML = "<p>Bedankt voor uw feedback! Uw gegevens:</br>"+
		"Naam: " + Voornaam + " " + Achternaam + "</br>"+ 
		"Telefoonnummer: " + Telefoonnummer +  " </br> Emailadres: " + Emailadres + "</p> <p> Uw bericht was: </br>" + Bericht + "</p>";
		document.getElementById("form-wrap").style.color = : "white";
		
	}
	
	function redirect(){
	document.location.href = "films/" + document.getElementById("search").value + ".html";
	return false;
	}


	</script>
